<table>
  <tr>
    <th>Name first</th>
    <th>Name middle</th>
    <th>Name last</th>
    <th>Password</th>
    <th>Username</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name_first %></td>
    <td><%= user.name_middle %></td>
    <td><%= user.name_last %></td>
    <td><%= user.username %></td>
    <td><%= user.password_digest %></td>
    <td><%= user.email %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

